<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sohail.aziz.broadcastExample"
    android:versionCode="1"
    android:versionName="1.0" >
    <permission 
        android:name="MYPERMISSION"
       
        ></permission>

    <uses-sdk android:minSdkVersion="9" />
    <uses-permission android:name="MYPERMISSION"/>
    
  

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".BroadcastExampleActivity"
            android:label="@string/app_name" >
          
        </activity>
      
        <activity
            android:name=".receiverActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".IndependentReceiver"
            android:enabled="true" 
  
         
             >
            <intent-filter>
                <action android:name="sohail.aziz.r2" />
            </intent-filter>
      
            </receiver>

        <service
            android:name=".MyIntentService"
            android:exported="false" >
        </service>
    </application>

</manifest>